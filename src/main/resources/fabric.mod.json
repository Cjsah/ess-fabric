{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Cjsah"
  ],
  "contact": {
    "homepage": "https://github.com/Cjsah",
    "sources": "https://github.com/Cjsah/fabric-example-mod"
  },

  "license": "GPL-3.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
